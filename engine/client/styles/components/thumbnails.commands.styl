@require '../lib.import.styl'

.thumbnails
  .command-container
    absolute right 10px bottom 10px

  .command-button
    display inline-block
    cursor pointer
    padding 8px
    margin-left 5px
    background-color #fff
    border-radius 50%
    &:hover
      box-shadow: 0px 0px 0px 4px rgba(0, 0, 0, 0.1);